  if test x$termcap_prefix != x; then
    LDFLAGS=$LDFLAGS_save
    if test x$LIBTERMCAP != xbroken; then
      INCTERMCAP=-I$termcap_prefix/include
      LIBTERMCAP=-L$termcap_prefix/lib
    fi
  fi
  AC_SUBST([LIBTERMCAP])
  AC_SUBST([INCTERMCAP])
])

